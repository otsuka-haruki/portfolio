generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("PLANETSCALE_DATABASE_URL")
  referentialIntegrity = "prisma"
}

// *: models
model Guestbook {
  id       Int    @id @default(autoincrement())
  name     String
  message  String
  approved String
  date     String
}

